<?xml version="1.0" encoding="utf-8"?>

<!-- 正向小按钮样式 -->
<selector xmlns:android="http://schemas.android.com/apk/res/android">

    <!-- 正常状态 -->
    <item android:state_pressed="false">
        <shape android:shape="rectangle">
            <solid android:color="@color/bg_btn_positive_small_normal"/>
            <corners
                android:radius="@dimen/btn_small_corner"/>
            <stroke
                android:width="@dimen/btn_small_border"
                android:color="@color/bd_btn_positive_small"/>
        </shape>
    </item>
</selector>